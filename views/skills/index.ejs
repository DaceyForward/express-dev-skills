<%- include('../partials/header') %>

  <h2>Skills as of <%= time %></h2>
    <ul>
      <% skills.forEach(function(t) { %>
        <li>
          <form action="/skills/<%= t.id %>?_method=DELETE"
            class="delete-form" 
            method="POST"
          >
            <button type="submit">X</button>
          </form>
          <a href="/skills/<%= t.id %>"><%= t.skill %></a>
            - 
          <%= t.mastered ? 'Yes' : 'No' %>
        </li>
      <% }); %>
    </ul>
    <a href="/skills/new">Add Skill</a>

 <%- include('../partials/footer') %>